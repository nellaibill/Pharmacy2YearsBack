          <DataField>InvoiceDateTime</DataField>
        </Field>
        <Field Name="ProductName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ProductName</DataField>
        </Field>
        <Field Name="ExpDate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>ExpDate</DataField>
        </Field>
        <Field Name="BatchNo">
          <rd:TypeName>System.String</rd:TypeName>

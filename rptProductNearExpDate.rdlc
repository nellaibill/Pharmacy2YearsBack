  <DataSets>
    <DataSet Name="dsLowStock_StockDetails1">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsLowStock</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>StockDetails1TableAdapter</rd:TableAdapterName>
        <rd:TableName>StockDetails1</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     s.InvoiceNo, s.InvoiceDateTime, s.ProductName, s.ExpDate, s.BatchNo, s.CurrentQty, p.ProductGroup
FROM         StockDetails s INNER JOIN
                      ProductMaster p ON s.ProductName = p.ProductName  where ExpDate&lt;=@ExpDatePlus and CurrentQty&gt;0</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="InvoiceNo">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>InvoiceNo</DataField>
        </Field>
        <Field Name="InvoiceDateTime">

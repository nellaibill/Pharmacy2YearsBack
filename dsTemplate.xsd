            <MainSource>
              <DbSource ConnectionRef="PharmacyConnectionString (MySettings)" DbObjectName="SELECT DISTINCT t.ProductName ,t.Qty AS OriginalQty ,(SELECT SUM(qty) FROM DrugSlipDetails WHERE ProductName=t.ProductName AND BillNo=d.BillNo AND BillDate=d.BillDate) AS CaseQty ,d.MRP AS MRP FROM OTTemplate t LEFT OUTER JOIN DrugSlipDetails d ON t.ProductName = d.ProductName AND t.DrName = d.DrName AND t.CaseType = d.CaseType AND d.BillNo=@BillNo AND (YEAR(BillDate) = @BillYear) WHERE t.DrName=@DrName AND t.CaseType=@CaseType" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="False" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="False" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT ProductName
,OriginalQty
,CaseQty
,OriginalQty*MRP AS OriginalAmount
,CASE WHEN CaseQty&lt;OriginalQty THEN OriginalQty-CaseQty END AS Less
,CASE WHEN CaseQty&lt;OriginalQty THEN (OriginalQty-CaseQty)*MRP 
 WHEN ISNULL(CaseQty,0) = 0 THEN (OriginalQty*(SELECT TOP 1 MRP FROM InvoiceDetails WHERE ProductName = DerivedTbl.ProductName ORDER BY InvoiceDateTime DESC ))
 END AS LessAmount
,CASE WHEN CaseQty&gt;OriginalQty THEN CaseQty-OriginalQty 
	  WHEN CaseQty = 0 THEN OriginalQty END AS Excess
,CASE 
WHEN CaseQty&gt;OriginalQty THEN ((CaseQty-OriginalQty)*MRP)  END AS ExcessAmount
,MRP

 FROM 
(
SELECT DISTINCT 
t.ProductName
	,t.Qty AS OriginalQty
	,(SELECT SUM(qty) FROM DrugSlipDetails WHERE ProductName=t.ProductName AND BillNo=d.BillNo AND BillDate=d.BillDate) AS CaseQty
	,d.MRP AS MRP
	FROM OTTemplate t 
		LEFT OUTER JOIN DrugSlipDetails d
			ON t.ProductName = d.ProductName
				AND t.DrName = d.DrName
				AND t.CaseType = d.CaseType
				AND d.BillNo=@BillNo
				AND (YEAR(BillDate) = @BillYear) WHERE t.DrName=@DrName AND t.CaseType=@CaseType
	) AS DerivedTbl</CommandText>

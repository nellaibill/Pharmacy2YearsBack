  <DataSets>
    <DataSet Name="dsSalesReturnBetweenDate_DataTable1">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsSalesReturnBetweenDate</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>DataTable1TableAdapter</rd:TableAdapterName>
        <rd:TableName>DataTable1</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     s.ProductName, SUM(s.ReturnQty) AS Expr1, p.ProductGroup, SUM(s.MRP) AS Amount
FROM         SalesReturnDetails s INNER JOIN
                      ProductMaster p ON s.ProductName = p.ProductName
WHERE     (s.ReturnBillDate BETWEEN @FromDate AND @ToDate)
GROUP BY s.ProductName, p.ProductGroup</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ProductName</DataField>
        </Field>
        <Field Name="Expr1">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Expr1</DataField>
        </Field>
        <Field Name="ProductGroup">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>ProductGroup</DataField>
        </Field>
        <Field Name="Amount">
          <rd:TypeName>System.Double</rd:TypeName>
          <DataField>Amount</DataField>
        </Field>
      </Fields>
    </DataSet>

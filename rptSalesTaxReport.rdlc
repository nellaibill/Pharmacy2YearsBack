  <DataSets>
    <DataSet Name="dsAccounts_AccountTitle">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsAccounts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>AccountTitleTableAdapter</rd:TableAdapterName>
        <rd:TableName>AccountTitle</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select * from AccountTitle</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="MonthName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>MonthName</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsAccounts_DrugSlipDetails">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsAccounts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>DrugSlipDetailsTableAdapter</rd:TableAdapterName>
        <rd:TableName>DrugSlipDetails</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     BillDate, BillNo, SUM((Total * 100) / (100 + VAT)) AS SalesValue, SUM(Total - (Total * 100) / (100 + VAT)) AS CST, VAT
FROM         DrugSlipDetails
WHERE     (BillDate BETWEEN @FromDate AND @ToDate) and (Status = 'P')
GROUP BY BillDate, BillNo, VAT
ORDER BY BillNo
</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="BillDate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>BillDate</DataField>
        </Field>
        <Field Name="Total">
          <rd:TypeName>System.Double</rd:TypeName>
          <DataField>Total</DataField>
        </Field>
        <Field Name="StarBillNo">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>StarBillNo</DataField>
        </Field>

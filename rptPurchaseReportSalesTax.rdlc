  <DataSets>
    <DataSet Name="dsAccounts_Purchase">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsAccounts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>PurchaseTableAdapter</rd:TableAdapterName>
        <rd:TableName>Purchase</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     s.SupplierID, s.SupplierName, s.TINNo, i.InvoiceNo, i.InvoiceDateTime, i.CompanyDate, i.CompanyInvoiceNo,i.VAT, SUM((i.Total * 100) / (100 + i.VAT)) AS PurchaseValue, 
                      SUM(i.Total) AS Total, SUM(i.Total - (i.Total * 100) / (100 + i.VAT)) AS CST
FROM         SupplierMaster s INNER JOIN
                      InvoiceDetails i ON s.SupplierID = i.SupplierID
WHERE     (i.InvoiceDateTime BETWEEN @FromDate AND @ToDate)
GROUP BY s.SupplierId,s.SupplierName,s.TINNo,i.InvoiceNo, i.InvoiceDateTime, i.CompanyDate, i.CompanyInvoiceNo, s.TINNo, i.VAT
ORDER BY i.InvoiceNo</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>

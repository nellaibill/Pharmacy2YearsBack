  <DataSets>
    <DataSet Name="dsTemplate_OTTemplate">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsTemplate</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>OTTemplateTableAdapter</rd:TableAdapterName>
        <rd:TableName>OTTemplate</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT ProductName
,OriginalQty
,CaseQty
,OriginalQty*MRP AS OriginalAmount
,CASE WHEN CaseQty&lt;OriginalQty THEN OriginalQty-CaseQty END AS Less
,CASE WHEN CaseQty&lt;OriginalQty THEN (OriginalQty-CaseQty)*MRP END AS LessAmount
,CASE WHEN CaseQty&gt;OriginalQty THEN CaseQty-OriginalQty END AS Excess
,CASE WHEN CaseQty&gt;OriginalQty THEN (CaseQty-OriginalQty)*MRP END AS ExcessAmount
,MRP
,BillNo
,BillDateTime
,PatientName
,CaseType
,DrName
,PatientID
 FROM 
(
SELECT DISTINCT 
BillNo
,BillDateTime
,PatientName
,d.CaseType AS CaseType
,d.DrName AS DrName
,d.PatientID AS PatientID
,t.ProductName
	,t.Qty AS OriginalQty
	,(SELECT SUM(qty) FROM DrugSlipDetails WHERE ProductName=t.ProductName AND BillNo=d.BillNo AND BillDate=d.BillDate) AS CaseQty
	,d.MRP AS MRP
	FROM OTTemplate t 
		LEFT OUTER JOIN DrugSlipDetails d
			ON t.ProductName = d.ProductName
				AND t.DrName = d.DrName
				AND t.CaseType = d.CaseType
				AND d.BillNo=@BillNo
				AND (YEAR(BillDate) = @BillYear)
	) AS DerivedTbl</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
      </Query>

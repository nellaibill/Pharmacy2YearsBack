      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>dsTemplate</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>DrugSlipDetailsTableAdapter</rd:TableAdapterName>
        <rd:TableName>DrugSlipDetails</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select ProductName,Qty,Total
From DrugSlipDetails WHERE ProductName NOT IN 
(SELECT ProductName FROM OTTemplate 
WHERE DrName=(Select TOP 1 DrName FROM DrugSlipDetails WHERE BillNo=@BillNo AND YEAR(BillDate)=@BillYear) AND 
CaseType=(Select TOP 1 CaseType FROM DrugSlipDetails WHERE BillNo=@BillNo AND YEAR(BillDate)=@BillYear)) AND BillNo=@BillNo AND YEAR(BillDate)=@BillYear</CommandText>
        <DataSourceName>PharmacyConnectionString</DataSourceName>
